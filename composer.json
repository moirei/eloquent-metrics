{
    "name": "moirei/eloquent-metrics",
    "description": "Retrieve metric data on Eloquent Models for analytics.",
    "homepage": "https://moirei.github.io/eloquent-metrics",
    "license": "MIT",
    "authors": [
        {
            "name": "Augustus",
            "email": "augustusokoye@moirei.com"
        }
    ],
    "keywords":[
        "MOIREI",
        "Laravel",
        "eloquent",
        "metrics",
        "analytics",
        "charts",
        "chart.js",
        "chartisan"
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "laravel/helpers": "^1.4",
        "cakephp/chronos": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.17"
    },
    "autoload": {
        "psr-4": {
            "MOIREI\\Metrics\\": "src",
            "MOIREI\\Metrics\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
